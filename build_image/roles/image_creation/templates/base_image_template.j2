openchami_work_dir: "{{ openchami_work_dir }}"
rhel_base_image_name: "{{ rhel_base_image_name }}"
rhel_base_image: "{{ oim_node_name }}/{{ rhel_base_image_name }}"
cluster_name: "{{ oim_node_name }}"
cluster_domain: "{{ domain_name }}"
rhel_repos:
{% for repo in rhel_repos %}
  - { name: '{{ repo.name }}', url: '{{ repo.url }}', gpg: '{{ repo.gpg }}' }
{% endfor %}

base_image_package:
{% for pkg in base_image_pacakges %}
  - {{ pkg }}
{% endfor %}

base_image_commands:
{% for cmd in base_image_commands %}
  - {{ cmd | to_json }}
{% endfor %}